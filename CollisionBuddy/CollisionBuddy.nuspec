<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>CollisionBuddy</id>
		<version>2.1.1</version>
		<title>CollisionBuddy</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/CollisionBuddy</projectUrl>
		<licenseUrl>https://github.com/dmanning23/CollisionBuddy/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Monogame library for circle/line and tunnel-free circle/circle collision detection.</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.7"/>
				<dependency id="PrimitiveBuddy" version="2.0"/>
				<dependency id="RandomExtensions.dmanning23" version="2.0"/>
				<dependency id="Vector2Extensions" version="2.0"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.7"/>
				<dependency id="PrimitiveBuddy" version="2.0"/>
				<dependency id="RandomExtensions.dmanning23" version="2.0"/>
				<dependency id="Vector2Extensions" version="2.0"/>
			</group>
			
			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.WindowsDX" version="3.7"/>
				<dependency id="PrimitiveBuddy" version="2.0"/>
				<dependency id="RandomExtensions.dmanning23" version="2.0"/>
				<dependency id="Vector2Extensions" version="2.0"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="CollisionBuddy.Android\bin\Release\CollisionBuddy.Android.pdb" target="lib\MonoAndroid10\CollisionBuddy.Android.pdb" />
		<file src="CollisionBuddy.Android\bin\Release\CollisionBuddy.Android.dll" target="lib\MonoAndroid10\CollisionBuddy.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="CollisionBuddy.iOS\bin\Release\CollisionBuddy.iOS.pdb" target="lib\Xamarin.iOS10\CollisionBuddy.iOS.pdb" />
		<file src="CollisionBuddy.iOS\bin\Release\CollisionBuddy.iOS.dll" target="lib\Xamarin.iOS10\CollisionBuddy.iOS.dll" />

		<!--WindowsDX-->
		<file src="CollisionBuddy.WindowsDX\bin\Release\CollisionBuddy.WindowsDX.pdb" target="lib\net451\CollisionBuddy.WindowsDX.pdb" />
		<file src="CollisionBuddy.WindowsDX\bin\Release\CollisionBuddy.WindowsDX.dll" target="lib\net451\CollisionBuddy.WindowsDX.dll" />

	</files>
</package>
